{
    "name": "werxe/laravel-address",
    "description": "Address management for Eloquent models.",
    "keywords": [ "php", "laravel", "address", "addresses", "country", "countries", "province", "provinces" ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Bruno Gaspar",
            "email": "bruno@werxe.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "~5.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": { "Werxe\\LaravelAddress\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Werxe\\LaravelAddress\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
